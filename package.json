    "cross-env": "^5.1.4",
    "gh-pages": "^2.0.1",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-scripts-ts": "^3.1.0",
    "redux": "^4.0.1",
    "rollup": "^0.66.6",
